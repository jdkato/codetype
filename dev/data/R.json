{
    "keywords": [
        "TRUE", "FALSE", "frame", "Inf", "NaN",
        "NA_integer_", "NA_real_", "NA_complex_", "NA_character_", "NULL",
        "NA", "seq(", "c(", "return(", "library(", "source(", "cbind(", "pdf(",
        "png(", "jpeg(", "rnorm(", "runif(", "list("
    ],
    "symbols": ["<-", "{", "}", "$", ";", ":", "::"],
    "first_line": [
        "^(library|context|source|isolate)\\(.*(\\))?$", "^#!.*Rscript$",
        "^.{1,} <- .*\\(.{1,}$"
    ],
    "unique": ["<<-", "ifelse", "...", "function("],
    "flags": ["@", "=>"],
    "comments": ["#"]
}
