{
    "tokens": [
        "is", "alias", "class", "private", "template", "auto", "static",
        "foreach", "this", "{", "}", ";", "@", "~", "!", "=>", "std", "File(",
        "immutable"
    ],
    "first_line": [
        "^module\\s.+;$", "^deprecated(.*)$", "^Ddoc$", "^(public )?import .*;$",
        "^#!.*(-run|rdmd)$"
    ],
    "unique": [
        "inout", "safe", "pure", "unittest", "nogc", "Typedef", "writefln(",
        "Typedef!"
    ],
    "flags": [
        "NULL", " . ", "malloc(", "end", "boolean", "System", "Console",
        "var", "console", "val"
    ],
    "ignores": ["//", "/*", "/+", "\"", "'"]
}
