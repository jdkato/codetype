{
    "tokens": [
        "is", "alias", "class", "private", "template", "auto", "static",
        "foreach", "this", "{", "}", ";", "@", "~", "!", "=>"
    ],
    "first_line": [
        "^module .*;$", "^deprecated(.*)$", "^Ddoc$", "^(public )?import .*;$",
        "^#!.*(-run|rdmd)$"
    ],
    "unique": [
        "inout", "immutable", "safe", "pure", "unittest", "nogc", "Typedef",
        "writefln("
    ],
    "flags": ["NULL", " . ", "malloc("],
    "comments": ["//", "/*", "/+"]
}
