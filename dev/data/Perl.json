{
    "tokens": [
        "use", "sub", "defined", "unless", "until", "last", "redo", "get",
        "die", "print", "chomp(", "{", "}", ";", "$", "@", "->", "::", "=>",
        "%", " . "
    ],
    "first_line": [
        "^package\\s[^\"\\']*;$", "^use\\s[^\\{]*;$", "^#!.*perl$", "^my\\s.*$",
        "^sub\\s\\w*\\s{$"
    ],
    "unique": [
        "my", "eq", "ref", "shift", "FILEOUT", "=~", "1;", "FILE", "`",
        "foreach"
    ],
    "flags": ["fn", "pub", "impl"],
    "ignores": ["#", "=", "//", "'", "\"", "`", "%"]
}
