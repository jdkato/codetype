{
    "tokens": [
        "not", "then", "function", "and", "elseif", "nil", "repeat",
        "do", "until", "self", "true", "false", "#", "{", "}", "()", "end"
    ],
    "first_line": [
        "^#!.*lua(jit)?$", "^return {$", "^require ('|\")[^/]+('|\")$",
        "^function .*\\(.*\\)", "^local .{1,}$", "^return \\[\\[$"
    ],
    "unique": ["local", "~="],
    "flags": ["=~", "begin", "class"],
    "ignores": ["--", "--[[", "'", "\""]
}
