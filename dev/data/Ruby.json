{
    "tokens": [
        "alias", "begin", "defined", "ensure", "false", "nil",
        "redo", "not", "retry", "then", "true", "undef", "unless", "when",
        "each", "require", "module", "gets", "require_dependency",
        "Array", "Bignum", "Binding", "class", "File", "Fixnum", "Hash",
        "Method", "NilClass", "TrueClass", "has_key", "def", "!", "$", "|",
        "::", "@", "=>", "end", "do", "...", "new"
    ],
    "first_line": [
        "^require(?:_relative|_dependency)? (?:'|\")[^\\.]+(?:'|\")$",
        "^<%.*%>$", "^module [^;\\s]+$", "^#!.*ruby$", "^class .*; end$",
        "^class .* < .*::.*$", "^.+::.+ = .+$", "^(?!for).+ .+ do$"
    ],
    "flags": [
        "function", "val", "import", "tell", "include(", "const", "printsum(",
        "@printf", "println(", "let", "set"
    ],
    "unique": [
        "elsif", "chomp", "to_i", "to_f", "rescue", "puts", "%w", "?",
        "attr_reader"
    ],
    "ignores": ["#", "=", "'", "\"", "`", "%w["]
}
