{
    "keywords": [
        "global", "use", "namespace", "class", "extends", "public",
        "function", "exit", "require(", "define(", "require_once(", "unset(",
        "empty(", "strpos(", "header(", "exit"
    ],
    "symbols": [
        "{", "}", ";", "$", "@", "===", "->", "::", "=>", "!==", " . ", "$_"
    ],
    "first_line": [
        "^<\\?(php)?.*$", "^function .*\\(.*\\)", "^namespace [^\"'::]*;$",
        "^use .*\\{1,}.*$"
    ],
    "unique": [],
    "flags": ["my", "=~", "print", "fn"],
    "comments": ["//", "/*", "#"]
}
