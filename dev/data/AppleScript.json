{
    "tokens": [
        "end", "set", "from", "with", "to", "begins", "&", "property",
        "application", "activate", "of", "then", "run", "key", "code", "then",
        "is", "true", "false", "shell", "do", "script", "of"
    ],
    "first_line": [
        "^on run$", "^set.+to.+$", "^property .* ?: ?.*$", "^osascript.+$",
        "^tell .+$", "^#!.*osascript$", "^(?:on|to) .+\\(?.+$"
    ],
    "unique": [
        "tell", "choose", "display", "dialog", "delay", "without", "on",
        "repeat"
    ],
    "flags": ["local", "function(", "require", "nil", "val", "var", "->"],
    "ignores": ["--", "(*", "#", "\"", "'"]
}
