{
    "keywords": [
        "id", "self", "nil", "void", "alloc", "init"
    ],
    "symbols": ["{", "}", ";", "-", "@", "[", "]", "+"],
    "first_line": [
        "^#import (\".*\"|<.*>)$", "^#(if|ifdef|define|undef) .*$",
        "^#include (\"((?!\\.cpp\"|\\.cc\").)*\"|<.*>)$"
    ],
    "unique": [
        "NSObject", "NSInteger", "NSNumber", "NSArray", "NSString", "BOOL",
        "NO", "YES", "NSFileManager", "NSDictionary", "NSMutableArray",
        "NSMutableSet", "NSLog", "NSApplicationMain", "@implementation",
        "@end", "@protocol", "@property", "@interface", "NSURL", "NSError",
        "removeObjectAtIndex", "@selector", "respondsToSelector", "IBAction"
    ],
    "flags": ["free(", "strlen(", "argc", "argv"],
    "comments": ["//", "/*"]
}
