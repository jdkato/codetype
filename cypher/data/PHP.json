{
    "keywords": [
        "global", "use", "namespace", "class", "extends", "public",
        "function", "exit", "require(", "define(", "require_once(", "unset(",
        "empty(", "strpos(", "header("
    ],
    "symbols": [
        "{", "}", ";", "$", "@", "===", "->", "::", "=>", "!==", " . "
    ],
    "first_line": [
        "^<\\?(php)?$", "^function .*\\(.*\\)", "^(namespace|use) [^\"']*;$"
    ],
    "unique": ["$_"],
    "flags": [],
    "comments": ["//", "/*"]
}
