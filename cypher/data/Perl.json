{
    "keywords": [
        "use", "sub", "defined", "unless", "until", "last", "redo", "get",
        "die", "print", "chomp("
    ],
    "symbols": [
        "{", "}", ";", "$", "@", "->", "::", "=>", "%", " . "
    ],
    "first_line": [
        "^package [^\"\\']*;$", "^use .*;$", "^#!.*perl$", "^sub \\w* {$"
    ],
    "unique": [
        "my", "eq", "ref", "shift", "FILEOUT", "=~", "1;", "FILE", "`",
        "foreach"
    ],
    "comments": ["#", "="]
}